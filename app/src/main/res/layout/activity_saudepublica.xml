<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.EuAlugo.EuAlugoFragment">

    <TextView
        android:id="@+id/apresentacao_saude"
        android:text="Saúde Pública"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:layout_marginLeft="30dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        />

    <TextView
        android:id="@+id/horarios"
        android:layout_below="@+id/apresentacao_saude"
        android:text="Horários"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="30dp"
        android:textSize="13dp" />

    <View
        android:id="@+id/view"
        android:layout_below="@+id/horarios"
        android:layout_width="wrap_content"
        android:background="@color/black"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="5dp"
        android:layout_height="1dp" />



    </RelativeLayout>



